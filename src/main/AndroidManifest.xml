<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jennatauro.livefit" >

    <application
        android:allowBackup="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name"
            android:icon="@drawable/ic_launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.FitnessHome"
            android:label="@string/title_activity_fitness_home"
            android:icon="@drawable/ic_action_dumbbell">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jennatauro.livefit.MainActivity" />
        </activity>
        <activity
            android:name=".activities.WorkoutDetailsActivity"
            android:label="@string/title_activity_workout_details"
            android:icon="@drawable/ic_action_dumbbell">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jennatauro.livefit.FitnessHome" />
        </activity>
    </application>

</manifest>
